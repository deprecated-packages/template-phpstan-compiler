{
    "name": "symplify/template-phpstan-compiler",
    "type": "phpstan-extension",
    "description": "Internal package for template to PHPStan-compatible compilers",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symplify/astral": "^10.3",
        "symplify/package-builder": "^10.3",
        "phpstan/phpstan": "^1.4.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/easy-testing": "^10.3",
        "symplify/phpstan-extensions": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\TemplatePHPStanCompiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\TemplatePHPStanCompiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.3-dev"
        },
        "phpstan": {
            "includes": [
                "config/services.neon"
            ]
        }
    },
    "conflict": {
        "symplify/astral": "<9.4.69",
        "symplify/composer-json-manipulator": "<10.2.0",
        "symplify/easy-coding-standard": "<10.2.0",
        "symplify/phpstan-rules": "<10.2.0",
        "symplify/easy-testing": "<10.2.0",
        "symplify/rule-doc-generator-contracts": "<10.2.0",
        "symplify/php-config-printer": "<10.2.0",
        "symplify/autowire-array-parameter": "<10.2.0",
        "symplify/amnesia": "<10.2.0",
        "symplify/phpstan-extensions": "<10.2.0",
        "symplify/rule-doc-generator": "<10.2.0",
        "symplify/vendor-patches": "<10.2.0",
        "symplify/skipper": "<10.2.0",
        "symplify/smart-file-system": "<10.2.0",
        "symplify/symfony-static-dumper": "<10.2.0",
        "symplify/git-wrapper": "<10.2.0",
        "symplify/symplify-kernel": "<10.2.0",
        "symplify/monorepo-builder": "<10.2.0",
        "symplify/config-transformer": "<10.2.0",
        "symplify/easy-ci": "<10.2.0",
        "symplify/coding-standard": "<10.2.0",
        "symplify/package-builder": "<10.0.9",
        "symplify/latte-phpstan-compiler": "<10.2.0",
        "symplify/phpstan-latte-rules": "<10.2.0",
        "symplify/easy-parallel": "<10.2.0",
        "symplify/neon-config-dumper": "<10.2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

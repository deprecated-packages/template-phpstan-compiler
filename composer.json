{
    "name": "symplify/template-phpstan-compiler",
    "type": "phpstan-extension",
    "description": "Internal package for template to PHPStan-compatible compilers",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "nette/utils": "^3.2",
        "symplify/astral": "^10.3",
        "phpstan/phpstan": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symplify/easy-testing": "^10.3",
        "symplify/phpstan-extensions": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\TemplatePHPStanCompiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\TemplatePHPStanCompiler\\Tests\\": "tests"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config/config.neon",
                "config/services.neon"
            ]
        }
    }
}
